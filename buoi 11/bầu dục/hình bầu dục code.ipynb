    "import turtle as pen\n",
    "pen.speed(0)\n",
    "k=80\n",
    "a=0\n",
    "while (a<72):\n",
    "    pen.circle(k,90)\n",
    "    pen.circle(k//2,90)\n",
    "    if(a%2==0):\n",
    "        pen.rt(10)\n",
    "    a+=1"
